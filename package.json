{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "Starter Kit for web development using React, React Router, Redux & Bundling with Webpack & React Semantic UI",
  "scripts": {
    "postinstall": "patch-package",
    "start": "cross-env NODE_ENV=development node ./server.js",
    "start:debug": "cross-env NODE_ENV=development node --inspect-brk ./server.js",
    "start:prod": "cross-env NODE_ENV=production node ./server.js",
    "start:lint": "eslint --fix ./src --color",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack",
    "prestart": "npm run start:lint",
    "prebuild": "npm run start:lint"
  },
  "repository": {
    "type": "git",
    "url": "github.com/elios264/react-starter"
  },
  "author": "elios264",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.8.6",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "circular-dependency-plugin": "^5.0.2",
    "compression": "^1.7.4",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "env2": "^2.2.2",
    "eslint": "^6.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "^4.17.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "null-loader": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "patch-package": "^6.1.2",
    "path": "^0.12.7",
    "postcss-custom-media": "^7.0.8",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.0",
    "rimraf": "^2.6.3",
    "semantic-ui-less": "^2.4.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
